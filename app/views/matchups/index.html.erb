<p id="notice"><%= notice %></p>

<h1>Matchups</h1>

<table>
  <thead>
    <tr>
      <th>Blue team</th>
      <th>Red team</th>
      <th>Winner</th>
      <th>Loose</th>
      <th>Tournament</th>
      <th>Time</th>
      <th>Rank</th>
      <th>Create at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matchups.each do |matchup| %>
      <tr>
        <td><%= matchup.blue_team %></td>
        <td><%= matchup.red_team %></td>
        <td><%= matchup.winner %></td>
        <td><%= matchup.loose %></td>
        <td><%= matchup.tournament %></td>
        <td><%= matchup.time %></td>
        <td><%= matchup.rank %></td>
        <td><%= matchup.create_at %></td>
        <td><%= matchup.updated_at %></td>
        <td><%= link_to 'Show', matchup %></td>
        <td><%= link_to 'Edit', edit_matchup_path(matchup) %></td>
        <td><%= link_to 'Destroy', matchup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Matchup', new_matchup_path %>
